<h1>New Resume</h1>

<%#= render 'form', resume: @resume %>

<%= form_for @resume, html: { multipart: true } do |f| %>
  <%= f.label :user_id %>
  <%= f.text_field :user_id %>
  <%= f.label :file_content %>
  <%= f.file_field :file_content %>
  <%= f.submit "Save"%>
<% end %>

<%= link_to 'Resumes', resumes_path %>

